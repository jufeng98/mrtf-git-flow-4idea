<idea-plugin>
    <actions>
        <action id="GitFlowPlus.ServiceConsoleAction" class="com.github.xiaolyuh.action.ServiceConsoleAction">
            <add-to-group group-id="GitFlowPlus.Menu" relative-to-action="GitFlowPlus.FailureReleaseAction" anchor="after"/>
        </action>
        <action id="GitFlowPlus.ServiceConsoleSecAction" class="com.github.xiaolyuh.action.ServiceConsoleSecAction">
            <add-to-group group-id="GitFlowPlus.Menu" relative-to-action="GitFlowPlus.ServiceConsoleAction" anchor="after"/>
        </action>
        <action id="GitFlowPlus.ServiceLogAction" class="com.github.xiaolyuh.action.ServiceLogAction">
            <add-to-group group-id="GitFlowPlus.Menu" relative-to-action="GitFlowPlus.ServiceConsoleSecAction"
                          anchor="after"/>
        </action>
        <action id="GitFlowPlus.ServiceLogSecAction" class="com.github.xiaolyuh.action.ServiceLogSecAction">
            <add-to-group group-id="GitFlowPlus.Menu" relative-to-action="GitFlowPlus.ServiceLogAction"
                          anchor="after"/>
        </action>
    </actions>
</idea-plugin>